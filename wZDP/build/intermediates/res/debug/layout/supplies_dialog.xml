<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/pup_pop"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:gravity="center_horizontal"
    android:background="#F8F8FF"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/pop_layout"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

            <TextView
                android:id="@+id/start_month"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="起始月份"
                android:textAppearance="?android:attr/textAppearanceMedium"
                 android:focusable="false"  />

            <EditText
                android:id="@+id/month_ner"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium" />

            <TextView
                android:id="@+id/company_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="单位名称"
                android:textAppearance="?android:attr/textAppearanceMedium" />

            <EditText
                android:id="@+id/company_ner"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium" />

          
             <TextView
                android:id="@+id/project_type"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="项目类型"
                android:textAppearance="?android:attr/textAppearanceMedium" />

            <EditText
                android:id="@+id/typetext"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium" />

            <TextView
                android:id="@+id/Voltage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="电压等级"
                android:textAppearance="?android:attr/textAppearanceMedium" />

            <EditText
                android:id="@+id/Voltage_Class"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium" /> 
      
    </LinearLayout>
	<Button
	    android:id="@+id/select"
	    android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginTop="5dp"
		android:layout_marginLeft="200dp"
		android:text="查询"
        android:textStyle="bold"
	    />
</LinearLayout><!-- From: file:/Users/fony/Desktop/fony/as/asprojects/Android-Gradle-Examples-master/WZDP1/wZDP/src/main/res/layout/supplies_dialog.xml -->