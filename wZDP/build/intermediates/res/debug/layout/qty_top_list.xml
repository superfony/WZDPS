<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="HardcodedText"
    tools:context=".MovedActivity" >

	<RelativeLayout
	    android:id="@+id/relativeLayout1"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignParentLeft="true"
	    android:layout_alignParentRight="true"
	    android:layout_alignParentTop="true" >

	    <LinearLayout
	        android:id="@+id/fragment_left_container"
	        android:orientation="horizontal"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentBottom="true"
	        android:layout_alignParentTop="true" >

	        <fragment
	            android:id="@+id/fragment_left"
	            android:name="com.epsmart.wzdp.activity.supply.fragment.LeftFragment"
	            android:layout_width="match_parent"
	            android:layout_height="match_parent" />
	    </LinearLayout>

	    <LinearLayout
	        android:id="@+id/fragment_right_container"
	        android:orientation="horizontal"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentBottom="true"
	        android:layout_alignParentRight="true"
	        android:layout_alignParentTop="true"
	        android:layout_toRightOf="@+id/fragment_left_container"
	        >
	    </LinearLayout>
	</RelativeLayout>



</RelativeLayout>
